<h1 align="center">All Users</h1>
<% @users.each do |f| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li><%= link_to gravatar_for(f),user_path(f)%></li>
        <li class="article-titile">
          <%= link_to f.username, user_path(f)%>
        </li>
          <li><small> <%= pluralize(f.articles.count, "article") if f.articles %></small></li>
        </div>
      </div>
    </ul>
<% end %>